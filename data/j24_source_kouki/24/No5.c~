#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main(void){
  int n,i,j;
  int **arr;
  srand((unsigned)time(NULL));

  printf("How many?\n");
  scanf("%d",&n);

  printf("(1)");
  arr=(int **)malloc(sizeof(int)*n);
  for(i=0;i<n;i++);{
    arr[i)=(int *)malloc(sizeof(int)*n);
    for(j=0;j<n;j++){
      arr[j][i]=rand();
      arr[j][i]%=10;
      printf("%d ",arr[j][i]);
    }
    printf("\n");
  }
  return 0;
}
