#include<stdio.h>
#include<stdlib.h>
#include<time.h>

int main(void){

  int *pt;
  int i,n;

  srand = ((unsigned)time(NULL));

  printf("input number =");
  scanf("%d",&n);

  pt = (int *)malloc(sizeof(int)*n);

  for(i=0;i<n;i++){
    pt[i] = rand()%100;
    printf("pt[%d] = %d",i,pt[i]);
  }

  free(pt);

  return 0;
}
    
