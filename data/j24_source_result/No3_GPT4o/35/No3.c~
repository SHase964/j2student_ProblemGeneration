#include <stdio.h>
#define NUM 3
#define BOM 2

int main(void)
{
  int array[NUM]={3,1,4}:
  int arry[BOM]={2,5};
  int ary [NUM+BOM];
  
  for(int i=0; i<NUM; i++){
    ary[i]=array[i];
  }

  for(int k=0; i<BOM; k++){
    ary[i]=arry[k];
  }

  for(int d=0; d<i; d++){
    if(ary[i]<ary[i+1]){
      ary[i]=ary[i+1];
    }
  }

  printf("\n");
  for(i=0; i<NUM+BOM; i++){
  printf("%d",ary[i]);
  }
    
    
    
  

                      






  return 0;
}
